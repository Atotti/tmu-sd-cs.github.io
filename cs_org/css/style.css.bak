  -webkit-transform: translateY(-2px);
  transform: translateY(-2px);
}

.l-header__nav__button:hover span:nth-child(3) {
  -webkit-transform: translateY(2px);
  transform: translateY(2px);
}

.l-header__nav__button.is-open span:nth-child(1) {
  -webkit-transform: translateY(7px) rotate(45deg);
  transform: translateY(7px) rotate(45deg);
}

.l-header__nav__button.is-open span:nth-child(2) {
  opacity: 0;
}

.l-header__nav__button.is-open span:nth-child(3) {
  -webkit-transform: translateY(-7px) rotate(-45deg);
  transform: translateY(-7px) rotate(-45deg);
}

